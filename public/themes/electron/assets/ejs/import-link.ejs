<% 
    let views = ejse.data('views');
    Object.keys(views).map((e, i)=>{
        let item = views[e];
        let path_sorter = /sections\/(.)*\/(.)*.ejs/i.exec(item)[0];
        let is_partial = /partial/i.exec(item);
        let is_about = /about/i.exec(item);
        if ((path_sorter == 'sections/hand-trade-view/index.ejs'
            || path_sorter == 'sections/hand-trade-view/add.ejs'
            || path_sorter == 'sections/hand-trade-view/edit.ejs'
            || path_sorter == 'sections/account-api/index.ejs'
            || path_sorter == 'sections/account-api/add.ejs'
            || path_sorter == 'sections/account-api/edit.ejs'
            || path_sorter == 'sections/history-hand-trade/index.ejs'
            || path_sorter == 'sections/about/index.ejs'
            || path_sorter == 'sections/dashboard/index.ejs'
            || path_sorter == 'sections/balance-wallet/index.ejs'
            || path_sorter == 'sections/video-training/index.ejs'
            ) && !is_partial){     
            // hand-trade-view-add-section
            
            let path_join = (is_about) ? 'about-modal' : (path_sorter.split('/')[1] + '-' + path_sorter.split('/')[2]).split('.')[0] + '-section';
        %>
        <link rel="import" data-link="<%- path_join %>" href="<%-path_sorter%>">
        <%
        }
    });
%>
